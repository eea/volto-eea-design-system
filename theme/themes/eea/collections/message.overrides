/*******************************
         Theme Overrides
*******************************/

.ui.message {
  border-radius: @borderRadius !important;
  box-shadow: @boxShadow !important;
}

.ui.icon.message > .icon {
  font-size: @iconSize !important;
}

.ui.message .content {
  font-size: @12px;
}

.ui.attached.message {
  margin: 0;
}
