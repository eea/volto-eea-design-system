/*******************************
         Theme Overrides
*******************************/

/* Storybook example */
.col-example {
  padding: 10px;
  margin-bottom: 12px;
  background: @primaryColor;
  color: @white;
}

.ui[class*='any reversed'].grid,
.ui[class*='any reversed'].grid > .row,
.ui.grid > [class*='any reversed'].row {
  flex-direction: row-reverse;
}
