/*******************************
            Blockquote
*******************************/
/* Body */
@blockquoteBorderLeft : 10px solid @secondaryColor;
@childrenBorderLeft   : none;

/* Quote */
@quoteColor      : @secondaryColor;
@quoteMargin     : @relative12px @relative30px;
@quoteColor      : @japaneseIndigo;
@quoteFontSize   : @h4;

/* Author */
@authorMarginTop : @relative16px;
@authorWeight    : @bold;
@authorFontSize  : @h5;
